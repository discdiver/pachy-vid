{
  "pipeline": {
    "name": "im_pipeline"
  },
  "input": {
    "atom": {
      "glob": "/*",
      "repo": "videos"
    }
  },
  "transform": {
    "cmd": [
      "python",
      "./frames.py",
      "/pfs/videos",
	    "/pfs/out"
     ],
    "image": "discdiver/frames:v1.38"
  },
  "parallelism_spec": {
    "constant": 2
  },
  "enable_stats": true
}
